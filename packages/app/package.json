{
  "name": "@k-suite/app",
  "version": "1.0.1",
  "description": "General Application Template",
  "main": "index.js",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "doc": "jsdoc -c jsdoc.json -t ./node_modules/ink-docstrap/template -R README.md -d './docs' -r --verbose ./src",
    "test:all": "mocha test/*.mocha.js --recursive src/**/*.spec.js --timeout 5000",
    "test:spec": "mocha --recursive src/**/*.spec.js --timeout 5000",
    "test": "npm run build:test && npm run test:all",
    "cover": "nyc --reporter=html --reporter=text mocha test/*.mocha.js --recursive src/**/*.spec.js --timeout 5000 && rm -rf ./.nyc_output",
    "bulid:clean": "rm -rf lib",
    "build": "npm run bulid:clean && NODE_ENV=development babel src -d lib --copy-files && del lib/**/__test__",
    "build:prod": "npm run bulid:clean && NODE_ENV=production babel src -d lib --copy-files && del lib/**/__test__",
    "build:test": "npm run bulid:clean && NODE_ENV=test babel src -d lib --copy-files && del lib/**/__test__",
    "build:watch": "NODE_ENV=development babel src -w -d lib --copy-files"
  },
  "author": "Rockie Guo <rockie@kitmi.com.au>",
  "license": "MIT",
  "repository": "https://github.com/kitmi/k-suite/tree/master/packages/app",
  "bugs": {
    "url": "https://github.com/kitmi/k-suite/issues"
  },
  "keywords": [
    "app",
    "worker"
  ],
  "nyc": {
    "exclude": [
      "babel.config.js",
      "test"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-decorators": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "@babel/register": "^7.0.0",
    "babel-plugin-contract": "^1.3.0",
    "babel-plugin-source-map-support": "^2.0.1",
    "capture-console": "^1.0.1",
    "del-cli": "^1.1.0",
    "ink-docstrap": "^1.3.2",
    "jsdoc": "^3.5.5",
    "luxon": "^1.4.3",
    "minimist": "^1.2.0",
    "mocha": "^5.2.0",
    "nyc": "^13.0.1",
    "shelljs": "^0.8.2",
    "should": "^13.2.3",
    "source-map-support": "^0.5.9",
    "supertest": "^3.3.0",
    "winston-daily-rotate-file": "^3.3.3",
    "winston-mongodb": "github:kitmi/winston-mongodb"
  },
  "dependencies": {
    "rk-config": "^2.0.0",
    "rk-utils": "^2.1.8",
    "winston": "^3.1.0",
    "winstonflight": "^1.0.4"
  }
}
