import 'types';

entity dictionary
  -- 'Dictionary Entity'

  with
    i18n({ field: "name", locales: LOCALES })
    i18n({ field: "description", locales: LOCALES })

  has
    code : text maxLength(10) -- 'Code'
    name -- 'Name'
    description : text maxLength(200) -- 'Description'

  key code
