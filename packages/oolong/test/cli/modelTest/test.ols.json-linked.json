                    ],
                    "displayName": "Email",
                    "optional": true
                },
                "mobile": {
                    "name": "mobile",
                    "type": "text",
                    "maxLength": 20,
                    "modifiers": [
                        {
                            "oolType": "Validator",
                            "name": "isMobilePhone",
                            "args": {
                                "oolType": "PipedValue",
                                "value": {
                                    "oolType": "ObjectReference",
                                    "name": "latest.locale"
                                },
                                "modifiers": [
                                    {
                                        "oolType": "Processor",
                                        "name": "stringDasherize"
                                    }
                                ]
                            }
                        },
                        {
                            "oolType": "Processor",
                            "name": "normalizeMobile"
                        }
                    ],
                    "subClass": [
                        "phone"
                    ],
                    "displayName": "Mobile",
                    "optional": true
                },
                "status": {
                    "name": "status",
                    "type": "enum",
                    "values": [
                        "inactive",
                        "active",
                        "disabled",
                        "forbidden",
                        "deleted"
                    ],
                    "default": "inactive",
                    "subClass": [
                        "userStatus"
                    ],
                    "displayName": "Status"
                },
                "createdAt": {
