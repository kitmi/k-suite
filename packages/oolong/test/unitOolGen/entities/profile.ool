entity profile
  -- "Profile"

  has
    gender : $association -- "Profile Gender"
    id : text maxLength(32) -- "Profile Id"
    user : $association -- "Profile User"

  associations
    refersTo 'gender' as 'gender'
    belongsTo 'user' as 'user'

  key id

  index
    [user] is unique
    [gender]
